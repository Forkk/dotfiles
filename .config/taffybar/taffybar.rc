style "default" {
    bg[NORMAL]   = "#2d2d2d"
    fg[NORMAL]   = "#dcdcdc"
    text[NORMAL] = "#dcdcdc"
}

style "notification-button" = "default" {
    text[NORMAL] = "#FF0000"
    fg[NORMAL]   = "#FF0000"
}

widget "Taffybar*" style "default"
widget "*NotificationCloseButton" style "notification-button"
gtk-font-name="Source Code Pro 9"
